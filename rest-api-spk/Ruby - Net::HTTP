form_data = [['AppID', '2rtg2z53r'],['APIKey', '378d4582ad3ed253057cafe9c70fae8b'],['SecretKey', 'd6uk5fd'],['Sound0', File.open('/C:/Users/nuc/Desktop/tmp/my/2.wav')],['Sound1', File.open('/C:/Users/nuc/Desktop/tmp/my/3.wav')]]
